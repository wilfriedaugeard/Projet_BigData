<div class="card mb-4">
    <div class="card-header">
        <i class="fas fa-table mr-1"></i>
        Classement par nombre de Tweets
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Classement</th>
                        <th>id</th>
                        <th>Nom</th>
                        <th>Nombre de Tweets</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let i=0; i<topTweet.length; i++){%> 
                        <tr>
                            <td><%=i+1%></td>
                            <td><%= topTweet[i][0]%> </td>
                            <td><%= topTweet[i][1]%> </td>
                            <td><%= topTweet[i][2]%> </td>
                        </tr>
                        <%}%>
                </tbody>
            </table>
        </div>
    </div>
</div>